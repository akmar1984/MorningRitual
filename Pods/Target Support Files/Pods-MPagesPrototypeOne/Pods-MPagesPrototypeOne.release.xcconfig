CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AMPopTip" "${PODS_ROOT}/Headers/Public/BFPaperButton" "${PODS_ROOT}/Headers/Public/FontAwesomeKit" "${PODS_ROOT}/Headers/Public/TAOverlay" "${PODS_ROOT}/Headers/Public/UIColor+BFPaperColors" "${PODS_ROOT}/Headers/Public/VBFPopFlatButton" "${PODS_ROOT}/Headers/Public/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AMPopTip" -isystem "${PODS_ROOT}/Headers/Public/BFPaperButton" -isystem "${PODS_ROOT}/Headers/Public/FontAwesomeKit" -isystem "${PODS_ROOT}/Headers/Public/TAOverlay" -isystem "${PODS_ROOT}/Headers/Public/UIColor+BFPaperColors" -isystem "${PODS_ROOT}/Headers/Public/VBFPopFlatButton" -isystem "${PODS_ROOT}/Headers/Public/pop"
OTHER_LDFLAGS = $(inherited) -ObjC -l"AMPopTip" -l"BFPaperButton" -l"FontAwesomeKit" -l"TAOverlay" -l"UIColor+BFPaperColors" -l"VBFPopFlatButton" -l"c++" -l"pop" -framework "CoreText" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods